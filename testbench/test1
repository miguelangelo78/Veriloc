module Mymodule {
	public:
		// The ports of the module: */
		reg  input a, b, c, d[7:0;8:0];
		wire output q;
		reg input data[7:0] = -8hu1x0zZ020FA; // Test literals */
		char a = -1;

		// Constructor: */
		Mymodule() {
			myvar = 0;
			q = a && b && c;
		}

		// Constructor overloading: */
		Mymodule(char operation) {
			switch(operation) {
			case '0':
				q = a && b && c; break;
			case '1':
				q = a || b || c; break;
			default:
				q = 0;
			}
		}

		int add(char c) {
			char q1 = q+1;
			q += c;
			if(q >= 10 && q1<10)
				q = 0 + 10 * 10;
		}

		// Sensitivity list: */
		always(a, negedge b,c, posedge clock) {
			add(c);
		}

	private:
		char myvar;
};

testbench Test {
	wire mywire;
	reg myreg;

	Mymodule mod;

	Test(int a, int x) {

	}

	// Sensivity list: */
	always(mod.a, mod.b, posedge mod.c) {
		LABELIF:
		if(mod.a == 1)
			LABEL: run();
		elsif(mod.b==2) {
			// Do nothingss
		}else if(mod.c==3)
			run_again();
	}
};

int main(int argc, char ** argv) {
	printf("Example code");
	return 0;
}
